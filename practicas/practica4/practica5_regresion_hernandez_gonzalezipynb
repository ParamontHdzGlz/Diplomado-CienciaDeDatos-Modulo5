   "source": [
    "def hyperparam_logistic(X, y, pipe, model, param_grid):\n",
    "    pipe = Pipeline(pipe.steps + [(\"model\", model)])\n",
    "    hp = RandomizedSearchCV(cv=4, \n",
    "                            param_distributions=param_grid,\n",
    "                            n_iter=5,\n",
    "                            scoring=\"r2\", \n",
    "                            verbose=10,\n",
    "                            error_score=-1000, \n",
    "                            estimator=pipe, \n",
    "                            n_jobs=-1,\n",
    "                            random_state=0)\n",
    "    hp.fit(X=X, y = y)\n",
    "    print(f\"R2: {hp.best_score_:,.2f}\")\n",
    "    return hp"
   ]
  },
